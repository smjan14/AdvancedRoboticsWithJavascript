var prize = [0.2, -0.4, 0.2]
var exit = [-0.1, 0.5, 0]

new Job({name: "claw_game",
		do_list: [Dexter.move_to([prize[0], prize[1], prize[2] + 0.5], [0, 0, -1])],
        			Dexter.move_to(prize, [0, 0, 1]),
                    Dexter.move_to([[0], [0], [0.3]], [1, 0, 0]),
                    Dexter.move_to([exit[0], exit[1], exit[2] + 0.5], [0, 0, -1])
                  
})
